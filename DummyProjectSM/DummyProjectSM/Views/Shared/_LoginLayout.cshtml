<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="https://fonts.googleapis.com/css?family=Sniglet:800" rel="stylesheet">
    <link href="~/Content/Site.css/" rel="stylesheet">
</head>
<body>
    <p class="center"><img src="~/Images/foodieslogonotext.png" class="loginlogo" /></p>
    <p class="center"><img src="~/Images/foodiestext.png" class="logintextlogo" /></p>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <center><p>&copy; @DateTime.Now.Year - foodies inc.</p></center>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
